{
    "name": "dccapi",
    "env": {
      "rpc": "@rpc",
      "privatekey": "@privatekey"
    },
    "routes": [
        {
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST",
                "Access-Control-Allow-Headers": "*"
            },
            "src": "/api/(.*)",
            "dest": "/api/$1.js",
            "status": 200
        }
]
}